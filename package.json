{
  "name": "listening-spaces",
  "version": "1.0.0",
  "description": "a directory of hi-fi listening spaces",
  "repository": "https://github.com/jamesbvaughan/listening-spaces",
  "author": "James Vaughan <james@jamesbvaughan.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "tailwind": "tailwindcss -i assets/css/input.css -o assets/css/output.css",
    "dev": "concurrently -n tailwind,hugo 'yarn tailwind --watch' 'hugo server'",
    "build": "yarn tailwind --minify && hugo --minify"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "tailwindcss": "^3.4.14"
  },
  "packageManager": "yarn@1.22.19"
}
